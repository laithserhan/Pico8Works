-- bland logo

function init_logo()
 gm=1
 gc=60 -- wait 2s
 gd=true
end

function update_logo()
 gc-=1
 if (gc<=0) init_title()
end

function draw_logo()
 rectfill(0,0,127,127,5)
 map(122,29,38,56,2,2) -- o
 map(124,28,56,48,2,3) -- b
 map(126,29,74,56,2,2) -- n
 map(125,31,66,72,3,1) -- soft
 print("obn-p02 ver 0.11",
   32,82,6)
end

-- title screen

function init_title()
 gm=2
 gd=true
end

function update_title()
 if (btnp(4) or btnp(5)) then
  init_game()
 end
end

function draw_title()
 cls()
 print("attack of buddha",
   32,32,15)
 print("press any button",
   32,88,7)
 print("2015.10 "..
   "programmed by obono",
   10,112,5)
end

-- game functions

function init_game()
 gm=3
 gc=60
 sfx(0)
 gd=true

 px=64 -- buddha x
 py=32 -- buddha y
 pt=90 -- buddha jump interval
 pa=pt/2 -- buddha jump phase
 pe=false -- game over?

 ac={} -- people table
 local i,j
 for i=1,8 do
  ac[i]={} -- for each row
 end
 ak=0 -- interval of new person

 for i=32,55 do
  for j=0,127 do
   sset(j,i,4)
  end
 end
 sc=0
 ms=0
end

function update_game()
 if (pe) then
  move_people()
  game_over()
 elseif (gc==0) then
  pa+=1
  handle_input()
  if (pa<=pt) then
   move_player()
   add_person()
   move_people()
   if (pa==pt) kill_people()
  end
  check_condition()
 else
  gc-=1
  if (gc==0) start_music()
 end
 gd=true
end

function rndi(x)
 return flr(rnd(x))
end

function start_music()
 music(0,0,3)
end

function handle_input()
 pv=0
 if (btn(0)) pv=-2
 if (btn(1)) pv=2
end

function move_player()
 px=mid(16,px+pv,112)
 local a=pa/pt-.5
 py=min(33+352*a*a,121)
end

function add_person()
 ak-=1
 if (ak>0) return
 local a={}
 local z=(90-pt)/70
 local y=rndi(8)+1
 local v=rndi(2)*2-1
 a.m=rndi(2)
 a.x=64-v*68
 a.y=y
 a.v=v*(rnd(z)+z/2+.5)
 a.d=rnd(4)
 add(ac[y],a)
 ak=pt*pt/128
end

function move_people()
 local t,a
 for t in all(ac) do
  for a in all(t) do
   a.x+=a.v
   if (rnd(16)<1) then
    sset(a.x,40+a.y,4)
   end
   if (a.x<-4 or a.x>=132) then
    del(t,a)
   end
  end
 end
end

function kill_people()
 local t,a
 for t in all(ac) do
  for a in all(t) do
   if (abs(a.x-px)<=16) then
    if (a.m==0) sc+=1
    if (a.m==1) ms+=1
    set_corpse(a)
    del(t,a)
   end
  end
 end
end

function set_corpse(a)
 local i,j,l,r,c
 for i=-1,1 do
  l=-rndi(3)-1+abs(i)
  r= rndi(3)+1-abs(i)
  for j=l,r do
   c=3-a.m
   if (rnd(8)<1) c=1
   sset(a.x+j,40+a.y+i,c)
  end
 end
end

function check_condition()
 if (pa>=pt+20) then
  if (ms>=50) then
   pe=true
  else
   if (pt>20) pt-=1
   pa=0
  end
 end
end

function game_over()
 if (btnp(4) or btnp(5)) then
  init_game()
 --elseif (cs>=128) then
 -- init_title()
 end
 --if (cs==8) sfx(1)
end

function draw_game()
 cls()
 local y=0
 if (pa>pt and pa<pt+10) y=pa%2
 camera(0,y)
 draw_background()
 draw_people()
 draw_player()
 camera()
 draw_strings()
end

function draw_background()
 map(0,0,0,0,16,16)
end

function draw_people()
 local t,a,s,d
 for t in all(ac) do
  for a in all(t) do
   s=16+a.m*4+
     (a.x*sgn(a.v)/2+a.d)%4
   d=(a.v<0)
   spr(s,a.x-4,104+a.y,1,1,d)
  end
 end
end

function draw_player()
 spr(12,px-16,py-32,4,4)
end

function draw_strings()
 print("score "..sc,6,6,7)
 print("miss  "..ms,6,12,7)
 if (gc>0) then
  print("ready?",52,61,7)
 end
 if (pe) then
  print("game over",46,61,7)
 end
end

-- pico-8 special functions

function _init()
 init_logo()
end

update_fn={
 update_logo,
 update_title,
 update_game
}

function _update()
 update_fn[gm]()
end

draw_fn={
 draw_logo,
 draw_title,
 draw_game
}

function _draw()
 if (gd) draw_fn[gm]() gd=false
end

