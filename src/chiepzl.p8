-- bland logo

function init_logo()
 gm=1
 gc=60 -- wait 2s
 gd=true
end

function update_logo()
 gc-=1
 if (gc<=0) init_title()
end

function draw_logo()
 rectfill(0,0,127,127,5)
 map(122,29,38,56,2,2) -- o
 map(124,28,56,48,2,3) -- b
 map(126,29,74,56,2,2) -- n
 map(125,31,66,72,3,1) -- soft
 print("obn-p03 ver 0.11",
   32,82,6)
end

-- title screen

function init_title()
 gm=2
 gd=true
end

function update_title()
 if (btnp(4) or btnp(5)) then
  init_game()
 end
end

function draw_title()
 cls()
 map(91,0,36,16,7,7)
 print("twisted pieces of "..
   "wisdom",16,80,8)
 print("press any button",
   32,100,7)
 print("2016.04 "..
   "programmed by obono",
   10,112,5)
end

-- game functions

function init_game()
 gm=3
 gc=60
 local i
 piece={}
 for i=1,10 do
  piece[i]={
   x=i,y=i,rot=0
  }
 end
 p=1
 sfx(0)
 gd=true
end

function update_game()
 if (false) then
  game_over()
 elseif (gc==0) then
  handle_input()
  move_player()
 else
  gc-=1
  if (gc==0) start_music()
 end
 gd=true
end

function rndi(x)
 return flr(rnd(x))
end

function start_music()
 music(0,0,3)
end

function handle_input()
 if (btnp(0)) piece[p].x-=1
 if (btnp(1)) piece[p].x+=1
 if (btnp(2)) piece[p].y-=1
 if (btnp(3)) piece[p].y+=1
 if (btnp(4)) piece[p].rot=(piece[p].rot+1)%8
 if (btnp(5)) p=p%10+1
end

function move_player()
end

function game_over()
 if (btnp(4) or btnp(5)) then
  init_game()
 --elseif (cs>=128) then
 -- init_title()
 end
 --if (cs==8) sfx(1)
end

function draw_game()
 local i
 cls()
 draw_background()
 for i=1,10 do
  draw_piece(
    piece[i].x*8+4,
    piece[i].y*8+4,
    i,piece[i].rot)
 end
 draw_player()
 draw_strings()
end

function draw_background()
 map(80,0,20,20,11,11)
end

col_piece = {
  9, 3, 5, 4, 2, 4,13, 3, 2, 9,
 15,11,13, 9, 8,14, 6,12, 4,10,
  7, 6, 6,10,14,15, 7, 6, 9,15,
}

function draw_piece(x,y,p,r)
 local cx,cy,cw,ch,fx,fy,i
 if p<4 then
  if r<4 then
   cx,cy,cw,ch=0,r*2,8,2
   fx,fy=3,1-r%2
  else
   cx,cy,cw,ch=(r-4)*2,8,2,8
   fx,fy=1-r%2,3
  end 
 else
  cx,cy=r%2*4,flr(r/2)*4
  cw,ch=4,4
  fx,fy=1+r%2,1+flr(r/2)%2
 end
 pal(1,0)
 for i=0,2 do
  pal(5+i,col_piece[p+i*10])
 end
 x-=fx*8 y-=fy*8
 map((p-1)*8+cx,cy,x,y,cw,ch)
 pal()
end

function draw_player()
end

function draw_strings()
 --print("score "..sc,6,6,7)
 if (gc>0) then
  print("ready?",52,61,7)
 end
 if (pe) then
  print("game over",46,61,7)
 end
end

-- pico-8 special functions

function _init()
 init_logo()
end

update_fn={
 update_logo,
 update_title,
 update_game
}

function _update()
 update_fn[gm]()
end

draw_fn={
 draw_logo,
 draw_title,
 draw_game
}

function _draw()
 if (gd) draw_fn[gm]() gd=false
end

